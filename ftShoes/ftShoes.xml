<application>
	<name>ftSensorsToTest</name>
	<dependencies>
		<port>/ft/ftShoe_Left_Front/analog:o</port>
		<port>/ft/ftShoe_Left_Rear/analog:o</port>
		<port>/ft/ftShoe_Right_Front/analog:o</port>
		<port>/ft/ftShoe_Right_Rear/analog:o</port>
	</dependencies>
	<module>
		<name>yarpdatadumper</name>
		<parameters>--name /dumper/ft/ftShoe_Left_Front/analog:o --type bottle </parameters>
		<node>localhost</node>
		<tag>data-dumperft-head-stateExt-o</tag>
	</module>
	<connection>
		<from>/ft/ftShoe_Left_Front/analog:o</from>
		<to>/dumper/ft/ftShoe_Left_Front/analog:o</to>
		<protocol>udp</protocol>
	</connection>
	<module>
		<name>yarpdatadumper</name>
		<parameters>--name /dumper/ft/ftShoe_Left_Rear/analog:o --type bottle </parameters>
		<node>localhost</node>
		<tag>data-dumperft-head-stateExt-o</tag>
	</module>
	<connection>
		<from>/ft/ftShoe_Left_Rear/analog:o</from>
		<to>/dumper/ft/ftShoe_Left_Rear/analog:o</to>
		<protocol>udp</protocol>
	</connection>
	<module>
		<name>yarpdatadumper</name>
		<parameters>--name /dumper/ft/ftShoe_Right_Front/analog:o --type bottle </parameters>
		<node>localhost</node>
		<tag>data-dumperft-head-stateExt-o</tag>
	</module>
	<connection>
		<from>/ft/ftShoe_Right_Front/analog:o</from>
		<to>/dumper/ft/ftShoe_Right_Front/analog:o</to>
		<protocol>udp</protocol>
	</connection>
	<module>
		<name>yarpdatadumper</name>
		<parameters>--name /dumper/ft/ftShoe_Right_Rear/analog:o --type bottle </parameters>
		<node>localhost</node>
		<tag>data-dumperft-head-stateExt-o</tag>
	</module>
	<connection>
		<from>/ft/ftShoe_Right_Rear/analog:o</from>
		<to>/dumper/ft/ftShoe_Right_Rear/analog:o</to>
		<protocol>udp</protocol>
	</connection>	
</application>